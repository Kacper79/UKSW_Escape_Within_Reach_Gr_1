{
  "metadata": [
    {
      "src": [
        {
          "files": ["Assets/Scripts/**/*.cs"],
          "exclude": ["**/obj/**", "**/bin/**"]
        }
      ],
      "dest": "api",
      "format": "yaml"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["Assets/Scripts/**/*.cs", "api/**.yml", "api/**.md"]
      }
    ],
    "dest": "_site"
  }
}